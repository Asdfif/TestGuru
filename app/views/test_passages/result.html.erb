<% title = @test_passage.test.title %>
<h1><%= t('.complete', title: title) %></h1>

<h2>
  <%= t('.success') %>
  - 
  <% percent = @test_passage.percent_of_success %>
  <% if @test_passage.success? %>
 <span class="success"><%= percent %>%</span>
  <%= t('.successfully', title: title) %>
  <% else %>
  <span class="fail"><%= percent %>%</span>
  <%= t('.failed', title: title) %>
  <% end %>
</h2>

<%= link_to t('.back'), tests_path %>